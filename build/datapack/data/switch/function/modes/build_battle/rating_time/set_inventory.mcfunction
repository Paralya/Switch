# @within	switch:modes/build_battle/rating_time/second
#

clear @s
item replace entity @s weapon.offhand with warped_fungus_on_a_stick[unbreakable={},item_model="minecraft:air"]
item replace entity @s hotbar.0 with brown_concrete[item_name={"text":"Super Poop","color":"#8B4513"}]
item replace entity @s hotbar.1 with red_concrete[item_name={"text":"Poop","color":"red"}]
item replace entity @s hotbar.2 with green_concrete[item_name={"text":"OK","color":"dark_green"}]
item replace entity @s hotbar.3 with lime_concrete[item_name={"text":"Good","color":"green"}]
item replace entity @s hotbar.4 with magenta_concrete[item_name={"text":"Epic","color":"light_purple"}]
item replace entity @s hotbar.5 with yellow_concrete[item_name={"text":"Legendary","color":"gold"}]

